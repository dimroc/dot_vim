{
  "codeLens.enable": true,
  "solargraph.useBundler": true,
  "suggest.minTriggerInputLength": 3,
  "suggest.autoTrigger": "none",
  "languageserver": {
    "elixir": {
      "command": "elixir-ls",
      "filetypes": ["elixir", "eelixir"]
    },
    "golang": {
      "command": "gopls",
      "filetypes": ["go"]
    }
  }
}
